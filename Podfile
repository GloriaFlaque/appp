# platform :ios, '9.0'

target 'Aplicacion' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  
  pod 'Firebase/Core'
  pod 'Firebase/Messaging'
  pod 'Firebase/Auth'
  pod 'Firebase/Firestore'
  pod 'Firebase/Database'
  pod 'Firebase/Storage'

  # Pods for Aplicacion

  target 'AplicacionTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'AplicacionUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
